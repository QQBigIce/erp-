<Window x:Class="AutoWrite.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoWrite"
        mc:Ignorable="d"
        Title="ERP自动录入" Height="450" Width="800" MinHeight="450" MinWidth="800"  WindowStyle="SingleBorderWindow" ResizeMode="CanResizeWithGrip">
    <Grid>
        <DockPanel Grid.Row="0">
            <TabControl DockPanel.Dock="Top" >
                <TabItem Header="输入">
                    <Frame Source="Home.xaml" />
                </TabItem>
                <TabItem Header="浏览器">
                    <Frame Source="WebBrowser.xaml" />
                </TabItem>
            </TabControl>
        </DockPanel>
    </Grid>



</Window>
